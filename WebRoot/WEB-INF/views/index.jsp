	/* Flash-start */
	var $images = $(".main-image li");
	var $button = $(".flicking-con a");
	var index = 0;
	var time = 0;
	function action() {
		$images.eq(index).fadeIn().siblings().fadeOut();
		$button.eq(index).addClass("btn-cur").siblings().removeClass("btn-cur");
		index = index + 1;
		if (index == $images.size())
			index = 0;
		time= window.setTimeout(action,4000); 
	}
	$button.click(function () {
		clearTimeout(time);
			index = $(this).index();
			action();
	});
	action();
	/* Flash-end */
</script>
